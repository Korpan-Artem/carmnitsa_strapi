{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "article_number": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "in_stock": {
      "type": "boolean",
      "default": true
    },
    "is_preorder": {
      "type": "boolean",
      "default": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand"
    },
    "model": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::model.model"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category"
    },
    "compatible_models": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::model.model"
    },
    "Card": {
      "type": "component",
      "component": "product.card",
      "repeatable": true
    }
  }
}
